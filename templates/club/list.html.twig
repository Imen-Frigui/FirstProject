<h1>La liste des formations </h1>
<h3>Le nombre de formation est {{f|length }}  </h3>
{% set maxV = 0 %}
{% for j in f %}
    {% set maxV= max(j.nb_participants, maxV) %}
  {% endfor %}
<table border="2">
    <tr>
        <th>Référence </th>
        <th>Titre </th>
        <th>Description </th>
        <th> NombreParticipants </th>
    </tr>
    {% for i in f %}
    {% if i.nb_participants == maxV %}
    {#{% if 'theorique' in i.Description %}#}
  <tr>
    <td> {{i.ref}}</td>
    <td> {{i.Titre | upper}}</td>
    <td> {{i.Description}}</td>
    <td> {{i.nb_participants}}</td>
  </tr>
  {% endif %}
  {% endfor %}
</table>
